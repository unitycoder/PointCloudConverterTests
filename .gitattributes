# point cloud files
*.las filter=lfs diff=lfs merge=lfs -text
*.laz filter=lfs diff=lfs merge=lfs -text

# output files
*.glb filter=lfs diff=lfs merge=lfs -text
*.bin filter=lfs diff=lfs merge=lfs -text
*.ucpc filter=lfs diff=lfs merge=lfs -text
*.pct filter=lfs diff=lfs merge=lfs -text
*.rgb filter=lfs diff=lfs merge=lfs -text
*.int filter=lfs diff=lfs merge=lfs -text
*.cla filter=lfs diff=lfs merge=lfs -text
